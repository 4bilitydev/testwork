<p id="notice"><%= notice %></p>

<h1>Schoolconnects</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schoolconnects.each do |schoolconnect| %>
      <tr>
        <td><%= schoolconnect.title %></td>
        <td><%= schoolconnect.content %></td>
        <td><%= link_to 'Show', schoolconnect %></td>
        <td><%= link_to 'Edit', edit_schoolconnect_path(schoolconnect) %></td>
        <td><%= link_to 'Destroy', schoolconnect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schoolconnect', new_schoolconnect_path %>
